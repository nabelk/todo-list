{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["main.js"],
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off",
        "no-use-before-define": [
            "error",
            {
                "functions": false
            }
        ],
        "no-alert": "off",
        "no-plusplus": "off",
        "no-return-assign": "off"
    },
    "extends": ["airbnb-base", "prettier"]
}
